socket.on("setIBB_status", (data, battery_qrcode) => {
    console.log("setIBB_status:", data);

    // Safety checks
    if (
        Array.isArray(data) &&
        data.length > 0 &&
        data[0].rows &&
        data[0].rows.length > 0
    ) {
        const ibbStatus = data[0].rows[0].ibb_status;

        if (ibbStatus === "NOT OK") {
            alert("The IBB Is Not Linked");
            location.reload();
        }
    }
});
