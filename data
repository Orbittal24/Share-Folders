Swal.fire({
    title: `IR Torque Pending of Pack: ${pack_no}!`,
    html: `
        <span style='font-size:20px;'>Pending Process:</span>
        <table id="table" border="1" style="width:100%; border-collapse:collapse; margin-top:10px;">
            <thead>
                <tr style='background-color:#e4eaf5;'>
                    <th colspan='4'>Incomplete Process</th>
                </tr>
                <tr>
                    <th>Station Name</th>
                    <th>Process Name</th>
                    <th>Status</th>
                    <th>Module No.</th>
                </tr>
            </thead>
            <tbody>
                ${incompleteData}
            </tbody>
        </table>
    `,
    icon: 'info',
    width: 800,
    showCancelButton: false,
    confirmButtonText: 'OK',

    allowOutsideClick: false,
    allowEscapeKey: false,
    allowEnterKey: false,
    stopKeydownPropagation: true,
    focusConfirm: false,

    didOpen: () => {
        const popup = Swal.getPopup();
        popup.addEventListener('keydown', function (e) {
            if (e.key === 'Enter') {
                e.preventDefault();
                e.stopImmediatePropagation();
                return false;
            }
        });
    }
});
