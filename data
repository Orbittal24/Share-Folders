var query_GLOBALDATA = `
SELECT 
    module_name,
    pack_name,
    Station_name,
    process_name,
    COUNT(process_name) AS processcount,
    SUM(total_screw) AS tot_screw_count
FROM 
    taco_treceability.taco_treceability.torque_masterEIP
WHERE 
    (
        (pack_name = '${pack_name}' AND Station_name = 'Pack_Assembly' AND module_name = '${module_num}')
        OR
        (Station_name = 'Tray_Assembly' AND module_name = '1')
    )
GROUP BY 
    process_name,
    Station_name,
    pack_name,
    total_screw,
    module_name
`;
